package ru.rknrl.core.kit {
public function plusMinus(a:Number, b:Number, epsilon:Number):void {
    if (a < b - epsilon) throw new Error(a + " != " + b);
    if (a > b + epsilon) throw new Error(a + " != " + b);
}
}
