<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/16.0">

    <id>ru.rknrl.castles</id>

    <filename>Castles</filename>

    <name>Castles</name>

    <versionNumber>0.0.1</versionNumber>
    <versionLabel>0.0.1</versionLabel>

    <supportedLanguages>en ru</supportedLanguages>

    <description>
        <text xml:lang="en">Castles description</text>
        <text xml:lang="ru">Башенки описание</text>
    </description>

    <copyright>Толя Янот</copyright>

    <extensions>
        <extensionID>org.onepf.OpenIAB</extensionID>
    </extensions>

    <icon>
        <image29x29>Icon-Small.png</image29x29>
        <image58x58>Icon-Small@2x.png</image58x58>

        <image50x50>Icon-Small-50.png</image50x50>
        <image100x100>Icon-Small-50@2x.png</image100x100>

        <image57x57>Icon.png</image57x57>
        <image114x114>Icon@2x.png</image114x114>

        <image72x72>Icon-Large.png</image72x72>
        <image144x144>Icon@2x-Large.png</image144x144>
    </icon>

    <initialWindow>
        <content>CastlesMobile.swf</content>

        <visible>true</visible>

        <autoOrients>false</autoOrients>

        <fullScreen>true</fullScreen>

        <renderMode>gpu</renderMode>
    </initialWindow>

    <iPhone>
        <InfoAdditions><![CDATA[
            <key>MinimumOSVersion</key>
			<string>5.0</string>

            <key>UIDeviceFamily</key>
            <array>
                <!-- iPhone and iPod touch devices -->
                <string>1</string>
                <!-- iPad devices -->
                <string>2</string>
            </array>

            <key>UIPrerenderedIcon</key>
            <true/>
        ]]></InfoAdditions>

        <Entitlements>
            <![CDATA[
                <key>aps-environment</key>
                <string>development</string>
            ]]>
        </Entitlements>

        <requestedDisplayResolution>high</requestedDisplayResolution>
    </iPhone>

</application>
